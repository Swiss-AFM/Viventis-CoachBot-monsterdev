{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node"
    }
  ],
  "functions": {
    "src/index.ts": {
      "maxDuration": 300,
      "memory": 3008
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.ts",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "PATCH",
        "DELETE",
        "OPTIONS"
      ]
    }
  ]
}